<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="459.0" prefWidth="540.0" style="-fx-background-image: url(&quot;mcbg.jpg&quot;);" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoadGameGUI">
    <right>
        <ScrollBar orientation="VERTICAL" prefHeight="348.0" prefWidth="20.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="20.0" right="20.0" top="20.0" />
            </BorderPane.margin>
        </ScrollBar>
    </right>
    <center>
        <ListView prefHeight="394.0" prefWidth="381.0" BorderPane.alignment="CENTER" fx:id="gameList">
            <BorderPane.margin>
                <Insets bottom="20.0" left="20.0" top="20.0" />
            </BorderPane.margin>
        </ListView>
    </center>
    <bottom>
        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="81.0" text="Back" BorderPane.alignment="CENTER" fx:id="backButtonAction" onAction="#backButtonAction">
            <BorderPane.margin>
                <Insets bottom="10.0" />
            </BorderPane.margin>
        </Button>
    </bottom>
    <left>
        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="Load Game" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets left="20.0" />
            </BorderPane.margin>
        </Button>
    </left>
</BorderPane>