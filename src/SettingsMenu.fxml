<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane style="-fx-background-image: url(&quot;mcbg.jpg&quot;);" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SettingMenuGUI" prefHeight="450.0" prefWidth="600.0">
   <center>
      <VBox alignment="CENTER" style="-fx-spacing: 10px;" BorderPane.alignment="CENTER">
         <children>
            <Label text="Music Volume">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Slider fx:id="musicSlider" scaleX="0.5" scaleY="0.8" scaleZ="0.5" />
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="25.0" prefWidth="85.0" text="Back" textAlignment="CENTER" />
         </children>
      </VBox>
   </center>
</BorderPane>
