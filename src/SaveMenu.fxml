<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane style="-fx-background-image: url(&quot;mcbg.jpg&quot;);" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SaveMenuGUI">
   <center>
      <VBox alignment="CENTER" style="-fx-spacing: 10px;" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="saveFile" promptText="file name" />
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonAction" prefHeight="25.0" prefWidth="85.0" text="Save" textAlignment="CENTER" />
               </children>
            </HBox>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="25.0" prefWidth="85.0" text="Back" textAlignment="CENTER" />
         </children>
      </VBox>
   </center>
   <top>
      <Label fx:id="saveGameLabel" text="Save Game" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
