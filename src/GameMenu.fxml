<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


      <BorderPane prefHeight="450.0" prefWidth="600.0" style="-fx-background-image: url(&quot;mcbg.jpg&quot;);" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameMenuGUI">
         <center>
            <VBox alignment="CENTER" prefHeight="551.0" prefWidth="894.0" style="-fx-spacing: 10px;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#newGameButtonAction" prefHeight="25.0" prefWidth="85.0" text="New Game" textAlignment="CENTER" />
                  <Button fx:id="loadButton" mnemonicParsing="false" onAction="#loadGameButtonAction" prefHeight="25.0" prefWidth="85.0" text="Load Game" textAlignment="CENTER" />
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="25.0" prefWidth="85.0" text="Back" textAlignment="CENTER" />
               </children>
            </VBox>
         </center>
      </BorderPane>


